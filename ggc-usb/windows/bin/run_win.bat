@echo off

REM  **********************************************************************
REM  ********   Set Java Path (calling setenv_java if exists)          ****
REM  **********************************************************************
if exist setenv_java_win.bat call setenv_java_win.bat


REM  **********************************************************************
REM  ********             Set ATech Tools Startup Jar                  ****
REM  **********************************************************************
call setenv_atech.bat


REM  **********************************************************************
REM  ********                Create Startup Files                      ****
REM  **********************************************************************
echo.  Create Startup files
java -classpath %ATECH_TOOLS_STARTUP% com.atech.update.startup.BuildStartupFile


REM  **********************************************************************
REM  ********                  Check Db Version                        ****
REM  **********************************************************************
call db_check.bat


REM  **********************************************************************
REM  ********              Run 'GNU Gluco Control'                     ****
REM  **********************************************************************
call run_ggc.bat




